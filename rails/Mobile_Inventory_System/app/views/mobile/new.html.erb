<%# if @mobile.errors %>
  <!-- <%#= @mobile.errors.count %> errors found! -->
   <%# @mobile.errors.messages.each do |key, error| %>
     <%#= key %><%#= error %>
   <%# end %>
<%# end %>
<%= form_for @mobile, url: {action: "create"} do |mobile| %>
  <label>MOBILE-NAME:			</label><%= mobile.text_field :name %><br />
   <label>MANUFACTURING_DATE:		</label><%= mobile.text_field :manufacturing_date %><br><br>
     .<%= mobile.collection_select :brand_id, @brands, :id, :name %>
  <%#= mobile.text_area :body, size: "60x12" %>
  <%= mobile.submit "Create" %>
<% end %>